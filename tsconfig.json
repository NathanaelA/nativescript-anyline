{
  "compilerOptions": {
	"module": "commonjs",
	"target": "es5",
	"declaration": false,
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
	"noEmitHelpers": true,
	"noEmitOnError": false,
	"skipLibCheck": true,
	"lib": [
	  "es6",
	  "dom",
	  "es2015.iterable"
	],
	"baseUrl": ".",
	"paths": {
	  "*": [
		"./node_modules/tns-core-modules/*",
		"./node_modules/*",
		"./demo/node_modules/tns-core-modules/*"
	  ]
	}
  },
  "exclude": [
	"demo",
	"demo-ng",
	"node_modules"
  ]
}